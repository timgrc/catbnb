<%= simple_form_for [@house, @reservation] do |f| %>
  <%= f.input :arrival_date, as: :date, html5: true, input_html: { value: session[:search_params]['arrival_date'] } %>
  <%= f.input :departure_date, as: :date, html5: true, input_html: { value: session[:search_params]['departure_date'] } %>
  <%= f.input :nb_cats, input_html: { value: session[:search_params]['nb_cats'] } %>
  <%= f.submit "Book", class: "btn btn-danger" %>
<% end %>
